{
  "name": "@howlin/bluetooth-hci-socket",
  "version": "0.5.3-6",
  "description": "Bluetooth HCI socket binding for Node.js.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/howlin/node-bluetooth-hci-socket.git"
  },
  "keywords": [
    "HCI",
    "Bluetooth",
    "Linux",
    "Windows"
  ],
  "author": "Sandeep Mistry <sandeep.mistry@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/howlin/node-bluetooth-hci-socket/issues"
  },
  "homepage": "https://github.com/howlin/node-bluetooth-hci-socket",
  "os": [
    "linux",
    "android",
    "freebsd",
    "win32"
  ],
  "dependencies": {
    "debug": "^4.1.1",
    "nan": "^2.14.0",
    "node-pre-gyp": "^0.14.0",
    "usb": "^1.6.2"
  },
  "optionalDependencies": {
    "usb": "^1.6.2"
  },
  "devDependencies": {
    "jshint": "^2.11.0"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "jshint lib/*.js && node test.js"
  },
  "binary": {
    "module_name": "bluetooth_hci_socket",
    "module_path": "./lib/binding/",
    "host": "https://github.com/howlin/node-bluetooth-hci-socket/releases/download/",
    "package_name": "{module_name}-{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "remote_path": "{version}"
  },
  "jshintConfig": {
    "esversion": 6
  },
  "directories": {
    "example": "examples",
    "lib": "lib"
  }
}
